{
    "name": "martin1982/live-broadcast-bundle",
    "type": "symfony-bundle",
    "description": "Bundle for live-streaming to various online networks",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin de Keijzer",
            "email": "martin.dekeijzer+live.broadcast.bundle@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-fileinfo": "*",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/orm": "^2.6",
        "facebook/graph-sdk": "^5.6",
        "google/apiclient-services": "^0.104",
        "google/apiclient": "^2.2",
        "psr/log": "^1.0",
        "react/event-loop": "^1.1",
        "symfony/console": "^3.4|^4.0",
        "symfony/framework-bundle": "^3.4|^4.0",
        "symfony/http-kernel": "^3.4|^4.0",
        "symfony/monolog-bundle": "^3.2",
        "symfony/validator": "^3.4|^4.0",
        "symfony/yaml": "^3.4|^4.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "escapestudios/symfony2-coding-standard": "^3.1",
        "friendsofphp/php-cs-fixer": "^2.10",
        "guzzlehttp/guzzle": "^6.3",
        "php-mock/php-mock-phpunit": "^2.4",
        "phpunit/phpunit": "^7.5",
        "sonata-project/doctrine-orm-admin-bundle": "^3.9",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "suggest": {
        "sonata-project/doctrine-orm-admin-bundle": "Sonata admin (version 3.x) for easy broadcast planning",
        "doctrine/doctrine-migrations-bundle": "Keep track of updated entities in your database"
    },
    "autoload": {
        "psr-4": { "Martin1982\\LiveBroadcastBundle\\": "" }
    }
}
